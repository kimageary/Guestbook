<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Pet Adoptions</h1>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Job Title</th>
                <th>Company</th>
                <th>LinkedIn</th>
                <th>Email</th>
                <th>Meet Options</th>
                <th>Other</th>
                <th>Message</th>
                <th>Mailing List</th>
                <th>Date Submitted</th>
            </tr>
        </thead>
        <tbody>
            <% entries.forEach(data => { %>
                <tr>
                    <td><%= data.id %></td>
                    <td><%= data.first_name %></td>
                    <td><%= data.last_name %></td>
                    <td><%= data.job_title %></td>
                    <td><%= data.company %></td>
                    <td><%= data.linkedIN %></td>
                    <td><%= data.email %></td>
                    <td><%= data.meet_options %></td>
                    <td><%= data.other %></td>
                    <td><%= data.message %></td>
                    <td><%= data.mailing_list %></td>
                    <td><%= data.date_submitted %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>